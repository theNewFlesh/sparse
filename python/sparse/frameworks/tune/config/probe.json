{
"probe_cli": {
	"target_path": "sparse/python/sparse/views/probe_cli.py",
	"line_width": 500,
	  "max_rows": 1000},
"probe": {
	"target_path": "sparse/bin/probe",
	"prompt": ["SpQL[", "]> "],
	"backingstores": {
		"qube": "<qube>"}
	},
"probe_api": {
	"custom_search_words": {
		"$2500"       : "(priority) <= (2500)",
		"$flrms"      : "(priority) <= (2500) & (flrms) > (0)",
		"$max"        : "(priority) <= (2500) & (max) >= (4)",
		"$long"       : "(priority) <= (2500) & (dist) <= (0.12)",
		"$down"       : "(state) = (down) & (name) !~ (sdc)",
		"$idle"       : "(state) = (idle) & (name) !~ (sdc)",
		"$idlefs"     : "(state) = (idle) & (slt_pct) >= (1) & (name) !~ (sdc)",
		"$down_sdc"   : "(state) = (down)",
		"$idle_sdc"   : "(state) = (idle)",
		"$idlefs_sdc" : "(state) = (idle) & (slt_pct) >= (1)"
		}
	},
"qube_backingstore" : {
	"input_lut"  : "master_lut",
	"output_lut" : "qube_long_name_lut"
	}
}